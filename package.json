{
  "name": "fullstack-backoffice",
  "version": "1.0.0",
  "type": "module",
  "description": "AdminJS Backoffice Panel",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "test:watch": "node test-watch-mode.js",
    "start:dev": "NODE_ENV=development ADMIN_WATCH_MODE=true node index.js",
    "start:prod": "NODE_ENV=production ADMIN_WATCH_MODE=false node index.js",
    "start:no-watch": "ADMIN_WATCH_MODE=false node index.js",
    "cache": "node ./routes/cache.js",
    "seed": "node ./prisma/seed.js",
    "build": "npm run build:css",
    "build:css": "postcss public/css/admin-custom.css -o public/css/admin-custom.css",
    "watch:css": "postcss public/css/admin-custom.css -o public/css/admin-custom.css --watch"
  },
  "dependencies": {
    "@adminjs/express": "^6.1.1",
    "@adminjs/prisma": "^5.0.4",
    "@adminjs/themes": "^1.0.1",
    "@faker-js/faker": "^9.9.0",
    "@fluentui/react": "^8.123.5",
    "@lit/react": "^1.0.8",
    "@mapbox/geojson-rewind": "^0.5.2",
    "@mapbox/togeojson": "^0.16.2",
    "adminjs": "^7.6.1",
    "antd": "^5.27.1",
    "bcryptjs": "^2.4.3",
    "chart.js": "^4.5.0",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "date-fns": "^2.30.0",
    "dayjs": "^1.11.13",
    "dotenv": "^16.3.1",
    "express": "^4.21.2",
    "express-session": "^1.18.0",
    "framer-motion": "^12.23.12",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-fs-backend": "^2.6.0",
    "iconv-lite": "^0.7.0",
    "jsonwebtoken": "^9.0.2",
    "lit": "^3.3.1",
    "lucide-react": "^0.539.0",
    "multer": "^2.0.2",
    "prisma": "^6.16.2",
    "react-chartjs-2": "^5.3.0",
    "react-datepicker": "^8.7.0",
    "react-dropzone": "^14.3.8",
    "react-leaflet": "^4.2.1",
    "recharts": "^3.2.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "swr": "^1.3.0",
    "thaidatepicker-react": "^2.1.3",
    "xmldom": "^0.6.0",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.20",
    "nodemon": "^3.0.1",
    "postcss": "^8.4.49",
    "postcss-cli": "^11.0.0",
    "tailwindcss": "^3.4.14"
  }
}
