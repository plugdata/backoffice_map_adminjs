{
  "name": "express-prisma-supabase",
  "version": "1.0.0",
  "type": "module",
  "description": "express + prisma + adminjs",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "concurrently \"npm run dev:css\" \"nodemon index.js\"",
    "build:css": "npx tailwindcss -i ./public/css/input.css -o ./public/css/admin-custom.css --config ./tailwind.config.js",
    "dev:css": "npx tailwindcss -i ./public/css/input.css -o ./public/css/admin-custom.css --config ./tailwind.config.js --watch",
    "build:css:min": "npx tailwindcss -i ./public/css/input.css -o ./public/css/admin-custom.min.css --config ./tailwind.config.js --minify",
    "prisma": "prisma",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:seed": "node prisma/seed.js",
    "db:reset": "prisma migrate reset --force",
    "db:studio": "prisma studio"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@adminjs/express": "^6.1.1",
    "@adminjs/leaflet": "^2.0.1",
    "@adminjs/prisma": "^5.0.4",
    "@adminjs/themes": "^1.0.1",
    "@adminjs/upload": "^4.0.2",
    "@lit/react": "^1.0.8",
    "@prisma/client": "^5.10.2",
    "adminjs": "^7.6.1",
    "bcryptjs": "^2.4.3",
    "chart.js": "^4.5.0",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "express": "^4.21.2",
    "express-session": "^1.18.0",
    "framer-motion": "^12.23.12",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-fs-backend": "^2.6.0",
    "jsonwebtoken": "^9.0.2",
    "lit": "^3.3.1",
    "lucide-react": "^0.539.0",
    "react-chartjs-2": "^5.3.0",
    "sonner": "^2.0.7"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.2.2",
    "esbuild": "^0.25.5",
    "nodemon": "^3.0.2",
    "postcss": "^8.5.3",
    "postcss-cli": "^11.0.0",
    "prisma": "^5.10.2",
    "tailwindcss": "^3.4.17"
  }
}
